{
	"name": "israfel-app",
	"version": "1.0.0",
	"private": true,
	"description": "Israfel App",
	"author": "FinBook Pte Ltd",
	"scripts": {
		"clean": "rimraf dist",
		"test": "jest",
		"tslint": "tslint -c tslint.json -p tsconfig.json src/**/*.ts{,x} --fix",
		"start": "webpack-dev-server --config webpack.config.local.js",
		"analyze": "npm run clean && npm run analyze:build && npm run analyze:no:build",
		"analyze:build": "webpack --config webpack.config.analyze.js",
		"analyze:no:build": "webpack-bundle-analyzer dist/stats.json",
		"rebuildDev": "webpack --config webpack.config.dev.js",
		"rebuild": "webpack --config webpack.config.uat.js",
		"rebuildLive": "webpack --config webpack.config.prod.js",
		"buildDev": "npm run clean && npm run rebuildDev",
		"build": "npm run clean && npm run rebuild",
		"buildLive": "npm run clean && npm run rebuildLive"
	},
	"jest": {
		"testEnvironment": "node",
		"globals": {
			"__ENV__": "dev",
			"window": {},
			"navigator": {
				"language": "EN"
			}
		},
		"setupFiles": [
			"<rootDir>/test-setup.js"
		],
		"moduleNameMapper": {
			"\\.(css|less|svg|png|jpg)$": "identity-obj-proxy"
		},
		"snapshotSerializers": [
			"<rootDir>/node_modules/enzyme-to-json/serializer"
		],
		"roots": [
			"<rootDir>/src"
		],
		"transform": {
			"^.+\\.tsx?$": "ts-jest"
		},
		"testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.tsx?$",
		"moduleFileExtensions": [
			"ts",
			"tsx",
			"js",
			"jsx",
			"json",
			"node"
		],
		"modulePaths": [
			"src"
		]
	},
	"devDependencies": {
		"@types/d3": "^5.7.0",
		"@types/enzyme": "^3.1.15",
		"@types/jest": "^23.3.12",
		"@types/lodash": "^4.14.119",
		"@types/react": "^16.7.18",
		"@types/react-dom": "^16.0.11",
		"@types/react-redux": "^7.0.0",
		"@types/react-router-dom": "^4.3.1",
		"@types/redux-logger": "^3.0.6",
		"@types/redux-mock-store": "1.0.0",
		"@types/ws": "^6.0.1",
		"browserslist": "^4.4.0",
		"coveralls": "^3.0.2",
		"css-loader": "^2.1.0",
		"enzyme": "^3.8.0",
		"enzyme-adapter-react-16": "^1.7.1",
		"enzyme-to-json": "^3.3.5",
		"file-loader": "^3.0.1",
		"html-webpack-plugin": "^3.2.0",
		"identity-obj-proxy": "^3.0.0",
		"jest": "^23.6.0",
		"mini-css-extract-plugin": "^0.5.0",
		"optimize-css-assets-webpack-plugin": "^5.0.1",
		"pre-commit": "^1.2.2",
		"react-test-renderer": "^16.7.0",
		"redux-mock-store": "^1.5.3",
		"rimraf": "^2.6.3",
		"style-loader": "^0.23.1",
		"terser-webpack-plugin": "^1.2.1",
		"ts-jest": "^23.10.5",
		"ts-loader": "^5.3.3",
		"tslint": "^5.12.1",
		"tslint-loader": "^3.6.0",
		"tslint-react": "^3.6.0",
		"url-loader": "^1.1.2",
		"webpack": "^4.28.4",
		"webpack-bundle-analyzer": "^3.0.3",
		"webpack-cli": "^3.2.1",
		"webpack-dev-server": "^3.1.14"
	},
	"dependencies": {
		"@babel/polyfill": "^7.2.5",
		"antd": "^3.12.3",
		"d3": "^5.7.0",
		"lodash": "^4.17.11",
		"moment": "^2.23.0",
		"react": "^16.7.0",
		"react-dom": "^16.7.0",
		"react-redux": "^6.0.0",
		"react-router-dom": "^4.3.1",
		"redux": "^4.0.1",
		"redux-logger": "^3.0.6",
		"redux-thunk": "^2.3.0",
		"reselect": "^4.0.0",
		"styled-components": "^3.4.10",
		"typescript": "^3.2.2"
	}
}
